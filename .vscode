{
  "window.zoomLevel": -1,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Railgun",
  "eslint.options": {
    // "configFile": ".eslintrc.js"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "files.associations": {
    "*Dockerfile*": "dockerfile",
    "*.cssx": "javascript",
    "*.js": "javascriptreact",
    ".env": "dotenv"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.meteor/local": true,
    "**/jobs/build": true,
    "**/jobs/imports": true,
    "**/elastic-proxy/build": true,
    "**/elastic-proxy/imports": true,
    "**/dump": false
  },
  "editor.formatOnSave": true,
  "saveAndRun": {
    "commands": [
      {
        "match": "@quadric/[^/]*/src/.*\\.js$",
        "cmd": "cd ${fileDirname} && cd .. && npm run build"
      }
    ]
  },
  "material-icon-theme.showUpdateMessage": false,
  "prettier.eslintIntegration": true
}